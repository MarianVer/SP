PROGRAM Program1;
VAR
  AAAAAAA, BBBBBBB, XXXXXX, YYYYYY, RRRRRR : LONGINT;
BEGIN
  SCAN(AAAAAAA);
  SCAN(BBBBBBB);

  PRINT(AAAAAAA + RRRRRR);
  PRINT(AAAAAAA - BBBBBBB);
  PRINT(AAAAAAA * BBBBBBB);
  PRINT(AAAAAAA DIV RRRRRR);
  PRINT(AAAAAAA MOD BBBBBBB);

  XXXXXX := (AAAAAAA - BBBBBBB) * 10 + (AAAAAAA + BBBBBBB) DIV 10;
  YYYYYY := XXXXXX MOD 10;

  PRINT(XXXXXX);
  PRINT(YYYYYY);
END.
