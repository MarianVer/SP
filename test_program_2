PROGRAM BranchAlg;
VAR
  A, B, C, MAX : LONGINT;
BEGIN
  SCAN(A); SCAN(B); SCAN(C);

  { найбільше число }
  IF (A >= B) AND (A >= C) THEN MAX := A
  ELSE IF (B >= A) AND (B >= C) THEN MAX := B
  ELSE MAX := C;
  PRINT(MAX);

  { усі числа однакові }
  IF (A = B) AND (B = C) THEN PRINT(1) ELSE PRINT(0);

  { хоча б одне від’ємне }
  IF (A < 0) OR (B < 0) OR (C < 0) THEN PRINT(-1) ELSE PRINT(0);

  { хоча б одне > 100 }
  IF (A > 100) OR (B > 100) OR (C > 100) THEN PRINT(1) ELSE PRINT(0);
END.
